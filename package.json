{
  "name": "ng-git-gen",
  "description": "Generates Angular (Module, Components and Routes) from static files in a git repo",
  "version": "0.0.18",
  "author": "Samuel Marks @SamuelMarks",
  "bin": {
    "ng-git-gen": "./bin/run"
  },
  "bugs": "https://github.com/SamuelMarks/ng-git-gen/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "async": "^3.2.0",
    "isomorphic-git": "^1.8.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@angular/common": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/async": "^3.2.5",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.51",
    "chai": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "rxjs": "^6.6.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/SamuelMarks/ng-git-gen",
  "keywords": [
    "oclif"
  ],
  "license": "(Apache-2.0 OR MIT)",
  "main": "lib/index.js",
  "oclif": {
    "bin": "ng-git-gen"
  },
  "repository": "SamuelMarks/ng-git-gen",
  "scripts": {
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
