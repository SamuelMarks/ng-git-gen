  },
  "devDependencies": {
    "@angular/common": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@angular/router": "^8.2.10",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/async": "^3.0.2",
