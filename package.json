    "isomorphic-git": "^1.8.0",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@angular/common": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/router": "^11.1.2",
    "@oclif/test": "^1.2.8",
    "@oclif/tslint": "^3.1.1",
    "@types/async": "^3.2.5",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.25",
    "chai": "^4.3.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
