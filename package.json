{
  "name": "ng-git-gen",
  "version": "0.0.16",
  "description": "Generates Angular (Module, Components and Routes) from static files in a git repo",
  "keywords": [
    "angular",
    "github",
    "github-wiki",
    "markdown",
    "md",
    "oclif",
    "wiki"
  ],
  "homepage": "https://github.com/SamuelMarks/ng-git-gen",
  "bugs": "https://github.com/SamuelMarks/ng-git-gen/issues",
  "repository": "SamuelMarks/ng-git-gen",
  "license": "MIT",
  "author": "Samuel Marks @SamuelMarks",
  "files": [
    "/bin",
    "/lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "ng-git-gen": "./bin/run"
  },
  "scripts": {
    "prepack": "rm -rf lib && tsc -b",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "tslint -p . -t stylish"
  },
  "dependencies": {
    "@angular/common": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@angular/platform-browser": "^8.2.10",
    "@angular/router": "^8.2.10",
    "@oclif/command": "^1.5.19",
    "@oclif/config": "^1.13.3",
    "@oclif/plugin-help": "^2.2.1",
    "async": "^3.1.0",
    "isomorphic-git": "^0.67.2",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular/compiler": "^8.2.10",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/async": "^3.0.2",
    "@types/chai": "^4.2.3",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.12",
    "@types/rimraf": "^2.0.2",
    "chai": "^4.2.0",
    "codelyzer": "^5.1.2",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "bin": "ng-git-gen"
  }
}
